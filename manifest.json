{
    "name": "SteamCode Chrome Extension", 
    "description": "Sample Chrome Extension", 
    "version": "2.0",
    "manifest_version": 3,
    "background": {
        "scripts": ["popup.js"]
    },
    "permissions": ["storage", "activeTab", "contextMenus"],
    "action": {
        "default_popup": "popup.html"
    }
}